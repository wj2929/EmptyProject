@model String
@using EmptyProject.Manage.Code
@{
     IList<SelectListItem> TriggersSelectListItems = GlobalController.TaskSchedulingConfig.ConfigEntity.Triggers.Select(t => new SelectListItem 
     {
         Text = t.Value.Name,
         Value = t.Key,
         Selected = t.Key == Model 
     }).ToList();
    }
@Html.DropDownListFor(model => model,TriggersSelectListItems)
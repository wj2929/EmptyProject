@model EmptyProject.Manage.Models.AddQuestionTemplateCategoryModel

@{
    ViewBag.Title = "ProvinceCityCategoryCreate";
    Layout = "~/Views/Shared/PopupForm.cshtml";
}
@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
	<div id="layOver" class="module_layover">
		<div class="module-c">
			<ul class="module_form">
				<li><span class="f_con">@Html.LabelFor(model => model.Name)
						@Html.TextBoxFor(model => model.Name, new { @class = "form_text" })
					</span>@Html.ValidationMessageFor(model => model.Name)</li>
	@Html.HiddenFor(model => model.ParentCategory_Id)
	@Html.HiddenFor(model => model.Type)
				<li><span class="f_con">@Html.LabelFor(model => model.OrderBy)
						@Html.TextBoxFor(model => model.OrderBy, new { @class = "form_text" })
					</span>@Html.ValidationMessageFor(model => model.OrderBy)</li>
                <li>
                    <span class="f_con">
                        @Html.LabelFor(model => model.Extended.TemplateContent)
                        @Html.TextAreaFor(model => model.Extended.TemplateContent, new { rows = "20", cols = "80", style = "width: 400px" })
                    </span>@Html.ValidationMessageFor(model => model.Extended.TemplateContent)
                </li>
                <li>
                    <span class="f_con">
                        @Html.LabelFor(model => model.Extended.CustomFormKey)
                        @Html.EditorFor(model => model.Extended.CustomFormKey)
                    </span>@Html.ValidationMessageFor(model => model.Extended.CustomFormKey)
                </li>
			</ul>
		</div>
		<div class="module-f"><input type="submit" value='提交' class='form_button button_add' /></div>
	</div>
}


<?xml version="1.0"?>
<RegularExpressionValidatorConfig>
  <Items>
    <Item>
      <Key>Email</Key>
      <Value>^[A-Za-z0-9](([_\.\-]?[a-zA-Z0-9]+)*)@([A-Za-z0-9]+)(([\.\-]?[a-zA-Z0-9]+)*)\.([A-Za-z]{2,})$</Value>
    </Item>
    <Item>
      <Key>无符号整数</Key>
      <Value>^[\p{N}]*$</Value>
    </Item>
    <Item>
      <Key>整数</Key>
      <Value>^[0-9-]*$</Value>
    </Item>
    <Item>
      <Key>数字（包括小数和负号）</Key>
      <Value>^[0-9.-]*$</Value>
    </Item>
    <Item>
      <Key>Guid</Key>
      <Value>^(\w{8}-(\w{4}-){3}\w{12})?$</Value>
    </Item>
    <Item>
      <Key>Url（http|https）</Key>
      <Value>^(((http|https)://)|(www\.))+(([a-zA-Z0-9\._-]+\.[a-zA-Z]{2,6})|([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}))(/[a-zA-Z0-9\&%_\./-~-]*)?$</Value>
    </Item>
    <Item>
      <Key>IP地址（IPV4）</Key>
      <Value>^(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9])\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9]|0)\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[1-9]|0)\.(25[0-5]|2[0-4][0-9]|[0-1]{1}[0-9]{2}|[1-9]{1}[0-9]{1}|[0-9])$</Value>
    </Item>
    <Item>
      <Key>带区号电话号码</Key>
      <Value>^\d{3,4}-?\d{6,8}$</Value>
    </Item>
    <Item>
      <Key>邮政编码</Key>
      <Value>^\d{6}$</Value>
    </Item>
    <Item>
      <Key>身份证号码（15位或18位）</Key>
      <Value>^([1-9]\d{7}((0\d)|(1[0-2]))(([0|1|2]\d)|3[0-1])\d{3}|[1-9]\d{5}[1-9]\d{3}((0\d)|(1[0-2]))(([0|1|2]\d)|3[0-1])((\d{4})|\d{3}[X]))$</Value>
    </Item>
  </Items>
</RegularExpressionValidatorConfig>
{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "manage": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/manage",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/assets/css/icons/icomoon/styles.css",
              "src/assets/css/bootstrap.css",
              "src/assets/css/core.css",
              "src/assets/css/components.css",
              "src/assets/css/colors.css",
              "node_modules/bootstrap-fileinput/css/fileinput.min.css",
              "node_modules/summernote/dist/summernote-lite.min.css"              
            ],
            "scripts": [
              "src/assets/js/core/libraries/jquery.min.js",
              "src/assets/js/core/libraries/bootstrap.min.js",
              "src/assets/js/plugins/notifications/bootbox.min.js",
              "src/assets/js/plugins/notifications/sweet_alert.min.js",
              "src/assets/js/plugins/pickers/anytime.min.js",
              "src/assets/js/plugins/pickers/pickadate/picker.js",
              "src/assets/js/plugins/pickers/pickadate/picker.date.js",
              "src/assets/js/plugins/pickers/pickadate/picker.time.js",
              "src/assets/js/plugins/forms/styling/uniform.min.js",
              "src/assets/js/plugins/forms/selects/bootstrap_multiselect.js",
              "src/assets/js/plugins/ui/moment/moment.min.js",
              "src/assets/js/plugins/pickers/daterangepicker.js",
              "src/assets/js/plugins/forms/selects/select2.min.js",
              "src/assets/js/plugins/forms/selects/select2tree.js",
              "src/assets/js/plugins/loaders/blockui.min.js",
              "src/assets/js/pages/layout_navbar_secondary_fixed.js",
              "node_modules/bootstrap-fileinput/js/plugins/piexif.min.js",
              "node_modules/bootstrap-fileinput/js/plugins/sortable.min.js",
              "node_modules/bootstrap-fileinput/js/plugins/purify.min.js",
              "node_modules/bootstrap-fileinput/js/fileinput.js",
              "node_modules/bootstrap-fileinput/themes/fa/theme.min.js",
              "node_modules/bootstrap-fileinput/js/locales/zh.js",
              "node_modules/summernote/dist/summernote-lite.min.js"              
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "manage:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "manage:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "manage:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "manage:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "manage:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "manage"
}